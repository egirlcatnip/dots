{
	"$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
	"modules": [
		{
			"type": "title",
			"format": "{1}@{2}",
			"key": " "
		},
		{
			"type": "separator",
			"length": 32
		},
		{
			"type": "host",
			"key": " ",
			"format": "{1}"
		},
		{
			"type": "os",
			"key": " ",
			"format": "{2} {9}"
		},
		{
			"type": "separator",
			"length": 32
		},
		{
			"type": "cpu",
			"key": " ",
			"format": "{1}"
		},
		{
			"type": "gpu",
			"key": " ",
			"format": "{2}"
		},
		{
			"type": "separator",
			"length": 32
		},
		{
			"type": "colors",
			"block": {
				"width": 4
			}
		}
	],
	"logo": {
		"type": "none"
	},
	"display": {
		"size": {
			"binaryPrefix": "si",
			"ndigits": 0
		},
		"hideCursor": true,
		"showErrors": true,
		"color": {
			"keys": "default",
			"title": "default"
		}
	}
}
